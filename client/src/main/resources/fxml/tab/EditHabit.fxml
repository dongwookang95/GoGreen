<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx"
            fx:controller="backend.controller.tab.EditHabitController" styleClass="two-size">
    <Pane id="two-edit-top-pane">
        <Label id="two-edit-cat-label" styleClass="two-edit-label" text="Category:"/>
        <ComboBox id="two-edit-cat-combo" styleClass="two-edit-combo" fx:id="categoryComboBox"/>
        <Label id="two-edit-sub-label" styleClass="two-edit-label" text="Subcategory:"/>
        <ComboBox id="two-edit-sub-combo" styleClass="two-edit-combo" fx:id="subcategoryComboBox"/>
        <Button id="two-edit-top-button" text="Edit" fx:id="edit"/>
    </Pane>
    <TableView id="two-edit-table-view" fx:id="view">
        <columns>
        </columns>
    </TableView>
    <Pane id="two-edit-bottom-pane" fx:id="bottomPane">
        <Button id="two-edit-bottom-button" text="Save" fx:id="save"/>
    </Pane>
</AnchorPane>
